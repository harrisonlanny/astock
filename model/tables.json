{
  "stock_basic": {
    "table_name": "stock_basic",
    "columns": [
      "ts_code CHAR(9) PRIMARY KEY",
      "symbol CHAR(6) NOT NULL",
      "name VARCHAR(20) NOT NULL",
      "area VARCHAR(8)",
      "industry VARCHAR(6)",
      "market VARCHAR(10)",
      "list_status CHAR(1)",
      "list_date DATE NOT NULL",
      "delist_date DATE",
      "is_hs CHAR(1)"
    ]
  },
  "d": {
    "table_name": "d_{symbol}",
    "columns": [
      "ts_code CHAR(9)",
      "trade_date DATE NOT NULL PRIMARY KEY",
      "open FLOAT(10)",
      "high FLOAT(10)",
      "low FLOAT(10)",
      "close FLOAT(10)",
      "pre_close FLOAT(10)",
      "change FLOAT(10)",
      "pct_chg FLOAT(10)",
      "vol FLOAT(10)",
      "amount FLOAT(10)",
      "adj_factor DOUBLE",
      "open_qfq FLOAT(10)",
      "high_qfq FLOAT(10)",
      "low_qfq FLOAT(10)",
      "close_qfq FLOAT(10)"
    ]
  },
  "announcements": {
    "table_name": "announcements",
    "columns": [
      "symbol CHAR(9)",
      "name VARCHAR(30)",
      "org_id VARCHAR(20)",
      "file_title VARCHAR(100) NOT NULL PRIMARY KEY",
      "title VARCHAR(80)",
      "url VARCHAR(80)"
    ]
  }
}